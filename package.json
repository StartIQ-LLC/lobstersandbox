{
  "name": "lobstersandbox",
  "version": "1.2.2",
  "description": "Safe sandbox launcher for OpenClaw - try OpenClaw without touching your real accounts",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "build:css": "./node_modules/.bin/tailwindcss -i ./styles/input.css -o ./public/tailwind.css --minify --content \"./views/*.js,./lib/*.js,./server.js\"",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testTimeout=10000"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.72.1",
    "autoprefixer": "^10.4.24",
    "cookie": "^1.1.1",
    "cookie-parser": "^1.4.7",
    "drizzle-zod": "^0.8.3",
    "express": "^4.22.1",
    "express-rate-limit": "^7.5.1",
    "http-proxy-middleware": "^3.0.5",
    "jest": "^30.2.0",
    "p-limit": "^7.2.0",
    "p-retry": "^7.1.1",
    "postcss": "^8.5.6",
    "supertest": "^7.2.2",
    "tailwindcss": "^4.1.18",
    "winston": "^3.19.0",
    "zod": "^4.3.6",
    "zod-validation-error": "^5.0.0"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "keywords": [
    "openclaw",
    "sandbox",
    "ai",
    "gateway"
  ],
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18"
  }
